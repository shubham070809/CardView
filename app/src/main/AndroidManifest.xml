<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cardview">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CardView">
        <activity android:name=".AndroidStudio"
            android:label="Android Studio"/>
        <activity android:name=".Java"
            android:label="Java"/>
        <activity android:name=".Shapes"
            android:label="Shapes"/>
        <activity android:name=".Aptitude"
            android:label="Aptitude"/>
        <activity android:name=".SolarSystem"
            android:label="Solar System"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>